{
  "name": "@highlight-run/sourcemap-uploader",
  "version": "0.5.2",
  "description": "Command line tool to upload source maps to Highlight",
  "bin": "./dist/index.js",
  "author": "Highlight",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/lib": {
      "require": "./dist/lib.js",
      "import": "./dist/lib.mjs",
      "types": "./dist/lib.d.ts"
    }
  },
  "scripts": {
    "typegen": "tsup src/index.ts src/lib.ts --dts-only",
    "build": "tsup src/index.ts src/lib.ts --format cjs,esm --dts"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "eslint": "^8.39.0",
    "npm-run-all": "^4.1.5",
    "tsup": "6.6.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "cross-fetch": "^3.1.5",
    "glob": "^9.3.0"
  }
}
