{
  "name": "@highlight-run/sourcemap-uploader",
  "version": "0.3.2",
  "description": "Command line tool to upload source maps to Highlight",
  "bin": "./dist/index.js",
  "author": "Highlight",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.0",
    "npm-run-all": "4.1.5",
    "tsup": "^6.2.3"
  },
  "dependencies": {
    "@types/node": "17.0.23",
    "aws-sdk": "2.1163.0",
    "cross-fetch": "^3.1.5",
    "eslint": "8.12.0",
    "glob": "7.2.3",
    "typescript": "^4.8.2",
    "yargs": "17.5.1"
  }
}
