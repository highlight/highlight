	"scripts": {
		"dev": "run-p --print-label --race 'dev:**'",
		"dev:vite": "vite",
		"dev:reflame": "node scripts/reflame.mjs --watch",
		"dev:tsm": "tsm src -w --logLevel=error",
		"dev:gql": "graphql-codegen --config --watch codegen.yml",
		"codegen": "graphql-codegen --config codegen.yml",
		"build": "yarn types:check && NODE_OPTIONS='--max-old-space-size=16384' vite build",
		"reflame": "yarn dev:reflame",
		"reflame-build": "node scripts/reflame.mjs",
		"lint": "eslint ./src",
		"lint:fix": "eslint ./src --fix",
		"sourcemaps": "yarn exec ../sourcemap-uploader/dist/index.js upload --apiKey \"${HIGHLIGHT_API_KEY}\" --appVersion \"${APP_VERSION}\" --path ./build",
		"test": "TZ=UTC vitest --run",
		"test:watch": "TZ=UTC vitest",
		"types:check": "tsc -noEmit",
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"svg": "svgr --typescript ./src/static --out-dir ./src/static --icon && yarn lint:es",
		"typegen": "tsc"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
