	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python File Debug",
			"type": "python",
			"request": "launch",
			"justMyCode": false,
			"program": "${file}",
			"console": "integratedTerminal",
		},
		{
			"name": "Attach to Process",
			"type": "go",
			"request": "attach",
			"debugAdapter": "dlv-dap",
			"mode": "remote",
			"port": 2345
		}
	]
}