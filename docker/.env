FRONTEND_IMAGE_NAME=ghcr.io/highlight/highlight-frontend:latest
KAFKA_IMAGE_NAME=confluentinc/cp-kafka:7.7.0
OTEL_COLLECTOR_BUILD_IMAGE_NAME=alpine:3.20.2
