CLICKHOUSE_ADDRESS
CLICKHOUSE_DATABASE=default
CLICKHOUSE_PASSWORD=
CLICKHOUSE_USERNAME=default
CONSUMER_SPAN_SAMPLING_FRACTION
DOPPLER_CONFIG=docker
EMAIL_OPT_OUT_SALT=salt
