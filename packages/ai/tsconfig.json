{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"moduleResolution": "node",
		"outDir": "dist",
		"strict": true,
		"baseUrl": "./",
		"esModuleInterop": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"useUnknownInCatchVariables": false
	},
	"include": ["src/*.ts"],
	"exclude": ["node_modules"]
}
