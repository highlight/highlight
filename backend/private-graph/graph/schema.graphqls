}

enum SocialType {
    Github
    LinkedIn
    Twitter
    Facebook
    Site
}

enum IntegrationType {
    Slack
    Linear
    Zapier
}

enum ErrorState {
    OPEN
    RESOLVED
    IGNORED
}

enum MetricType {
    WebVital
    Device
    Backend
}

enum AdminRole {
    ADMIN
    MEMBER
}

enum SessionCommentType {
    Admin
    FEEDBACK
