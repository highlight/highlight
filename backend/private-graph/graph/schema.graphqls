}

type BillingDetails {
    plan: Plan!
    meter: Int64!
    membersMeter: Int64!
    sessionsOutOfQuota: Int64!
}

type Invoice {
    amountDue: Int64
    amountPaid: Int64
    attemptCount: Int64
