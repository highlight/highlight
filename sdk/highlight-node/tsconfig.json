{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"declarationDir": "dist",
		"declarationMap": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["ESNext"],
		"module": "ESNext",
		"target": "ESNext",
		"moduleResolution": "Node",
		"skipLibCheck": true,
		"strict": true,
		"resolveJsonModule": true
	},
	"include": ["src"],
	"references": [{
		"path": "../firstload"
	}]
}
