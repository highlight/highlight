{
	"name": "@highlight-run/node",
	"version": "2.4.3",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"codegen": "graphql-codegen --config codegen.yml",
		"typegen": "tsup src/index.ts --dts-only",
		"build": "tsup src/index.ts --format cjs,esm --dts",
		"test": "jest"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@opentelemetry/api": "1.3.0",
		"@opentelemetry/auto-instrumentations-node": "^0.36.0",
		"@opentelemetry/exporter-trace-otlp-http": "^0.35.1",
		"@opentelemetry/sdk-node": "^0.35.1",
		"@opentelemetry/sdk-trace-base": "^1.9.1",
		"error-stack-parser": "2.0.7",
		"graphql": "^16.6.0",
		"graphql-request": "3.7.0",
		"graphql-tag": "2.12.6",
		"highlight.run": "workspace:*",
		"lru-cache": "^7.14.0",
		"npm-run-all": "4.1.5"
	},
	"devDependencies": {
		"@apollo/server": "^4.3.2",
		"@graphql-codegen/cli": "2.11.2",
		"@graphql-codegen/typescript": "2.8.1",
		"@graphql-codegen/typescript-graphql-request": "4.5.2",
		"@graphql-codegen/typescript-operations": "2.5.2",
		"@types/jest": "^29.2.0",
		"@types/lru-cache": "^7.10.10",
		"@types/node": "17.0.13",
		"apollo-server-plugin-base": "^3.7.1",
		"jest": "^29.2.2",
		"ts-jest": "^29.0.3",
		"tsup": "^6.2.3",
		"typescript": "^4.8.2"
	}
}
