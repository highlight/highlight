{
	"name": "@highlight-run/node",
	"version": "3.0.0",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"typegen": "tsup src/index.ts --dts-only",
		"dev": "yarn build --watch",
		"build": "tsup src/index.ts --format cjs,esm --dts",
		"test": "jest"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@opentelemetry/api": "^1.4.1",
		"@opentelemetry/auto-instrumentations-node": "0.36.5",
		"@opentelemetry/exporter-trace-otlp-http": "0.38.0",
		"@opentelemetry/resources": "^1.13.0",
		"@opentelemetry/sdk-node": "0.38.0",
		"@opentelemetry/sdk-trace-base": "1.12.0",
		"error-stack-parser": "2.0.7",
		"highlight.run": "workspace:*",
		"lru-cache": "^7.14.0",
		"npm-run-all": "4.1.5"
	},
	"devDependencies": {
		"@types/jest": "^29.2.0",
		"@types/lru-cache": "^7.10.10",
		"@types/node": "17.0.13",
		"jest": "^29.2.2",
		"ts-jest": "^29.0.3",
		"tsup": "^6.2.3",
		"typescript": "^5.0.4"
	}
}
