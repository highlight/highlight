{
	"name": "@highlight-run/node",
	"version": "3.12.1",
	"license": "Apache-2.0",
	"scripts": {
		"typegen": "tsc -d --emitDeclarationOnly",
		"dev": "yarn build --watch",
		"build": "rollup --config rollup.config.mjs",
		"test": "yarn test:check-deps && yarn test:vitest",
		"test:check-deps": "node scripts/check-deps.mjs",
		"test:vitest": "vitest run"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"types": "./dist/index.d.ts",
		"default": "./dist/index.cjs"
	},
	"files": [
		"dist"
	],
	"installConfig": {
		"hoistingLimits": "workspaces"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@prisma/instrumentation": ">=5.0.0",
		"require-in-the-middle": "^7.4.0"
	},
	"devDependencies": {
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/api-logs": "^0.57.2",
		"@opentelemetry/auto-instrumentations-node": "^0.56.0",
		"@opentelemetry/core": "^1.30.1",
		"@opentelemetry/exporter-jaeger": "^1.30.1",
		"@opentelemetry/exporter-logs-otlp-http": "^0.57.2",
		"@opentelemetry/exporter-metrics-otlp-http": "^0.57.2",
		"@opentelemetry/exporter-trace-otlp-http": "^0.57.2",
		"@opentelemetry/instrumentation": "^0.57.2",
		"@opentelemetry/otlp-exporter-base": "^0.57.2",
		"@opentelemetry/otlp-transformer": "^0.57.2",
		"@opentelemetry/resources": "^1.30.1",
		"@opentelemetry/sdk-logs": "^0.57.2",
		"@opentelemetry/sdk-node": "^0.57.2",
		"@opentelemetry/sdk-trace-base": "^1.30.1",
		"@opentelemetry/semantic-conventions": "^1.30.0",
		"@opentelemetry/winston-transport": "^0.10.0",
		"@rollup/plugin-commonjs": "^28.0.2",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^16.0.0",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^12.1.2",
		"@types/lru-cache": "^7.10.10",
		"@types/node": "^14.18.63",
		"check-dependencies": "^2.0.0",
		"encoding": "^0.1.13",
		"highlight.run": "workspace:*",
		"rollup": "^4.34.8",
		"typescript": "^5.0.4",
		"vitest": "^2.1.3"
	}
}
