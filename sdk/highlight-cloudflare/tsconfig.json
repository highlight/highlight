{
	"compilerOptions": {
		"types": ["@cloudflare/workers-types"],
		"target": "ES6",
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "ES6",
		"outDir": "dist",
		"removeComments": true,
		"declaration": true,
		"declarationDir": "dist",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"jsx": "react"
	},
	"include": ["src/**/*"]
}
