{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"lib": ["dom", "dom.iterable", "esnext"],
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"skipLibCheck": true,
		"outDir": "dist",
		"sourceMap": true,
	},
	"include": [
		"src/*.ts",
	],
}
